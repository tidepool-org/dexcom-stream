{
  "name": "tidepool-dexcom-stream",
  "version": "0.0.1",
  "description": "Parse Dexcom text files into json.",
  "main": "index.js",
  "scripts": {
    "test": "make test"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tidepool-org/dexcom-stream.git"
  },
  "keywords": [
    "Dexcom",
    "export",
    "text",
    "json",
    "parser",
    "stream"
  ],
  "testling": {
    "harness": "mocha",
    "browsers": [
      "ie/9..latest",
      "chrome/20..latest",
      "firefox/14..latest",
      "safari/latest",
      "opera/11.0..latest",
      "iphone/6",
      "ipad/6"
    ],
    "files": "test/Test*.js"
  },
  "config": {
    "blanket": {
      "pattern": [
        "index"
      ],
      "data-cover-never": [
        "node"
      ]
    }
  },
  "author": "Tidepool contributors",
  "license": "BSD-2",
  "bugs": {
    "url": "https://github.com/tidepool-org/dexcom-stream/issues"
  },
  "homepage": "https://github.com/tidepool-org/dexcom-stream",
  "devDependencies": {
    "should": "~3.1.2",
    "mocha-lcov-reporter": "0.0.1",
    "travis-cov": "~0.2.5",
    "mocha": "~1.17.1",
    "coveralls": "~2.7.0",
    "blanket": "~1.1.6"
  },
  "dependencies": {
    "event-stream": "~3.1.0",
    "moment": "~2.5.1"
  }
}
